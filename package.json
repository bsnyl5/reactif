{
  "name": "@oddx/reactive",
  "private": false,
  "version": "0.1.8",
  "description": "UI Reactive library",
  "main": "dist/index.js",
  "scripts": {
    "umd": "NODE_ENV=production parcel build src/index.ts --out-dir=umd --no-source-maps --global ReOdd --experimental-scope-hoisting",
    "build": "tsc --declaration && npm run umd --experimental-scope-hoisting",
    "test": "jest",
    "sim": "sh simulate.sh",
    "example": "parcel example/index.html --no-source-maps --out-dir=.tests",
    "netlify": "npm run build && npm run sim && parcel build example/index.html --out-dir=public"
  },
  "author": "Oddx Team",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
